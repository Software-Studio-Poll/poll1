<p id="notice"><%= notice %></p>

<%= render partial: "question", object: @question %>



<div class="col-md-8">
  <% pie_array = [] %>
  <% if @question.answerchoices.any? %>
    <% for i in @question.answerchoices do%>
      <% pie_array << [i.content, i.tally] %>
      <% #pie_array << [i.content, i.userchoices.size] %>
    <% end %>
    
    
    <% puts pie_array %>
  
     
     
     <%= pie_chart pie_array %>
     
  <% end %>
</div>




<%= link_to 'Edit Question text', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
